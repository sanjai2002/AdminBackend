{
  "serilog": {

    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],

    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/LXPLogs-.txt",
          "rollingInterval": "Day"
        }
      }
    ]

  },
  "AllowedHosts": "*"
}